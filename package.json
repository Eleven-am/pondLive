{
  "name": "pondsocket",
  "version": "0.1.128",
  "description": "PondSocket is a fast simple socket server",
  "keywords": [
    "socket",
    "server",
    "ws",
    "websocket",
    "PUB/SUB",
    "pubsub",
    "presence",
    "realtime",
    "realtime server"
  ],
  "scripts": {
    "test": "jest --coverage",
    "build:client": "parcel build src/pondclient/pondLive.ts --no-source-maps",
    "build:server": "tsc",
    "build": "npm run build:server && npm run build:client"
  },
  "author": "Roy OSSAI",
  "license": "GPL-3.0",
  "main": "bin/server.js",
  "types": "types/server.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eleven-am/pondSocket.git"
  },
  "exports": {
    ".": "./bin/server.js",
    "./client": "./bin/client.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./types/server.d.ts"
      ],
      "client": [
        "./types/client.d.ts"
      ]
    }
  },
  "dependencies": {
    "rxjs": "^7.5.6",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@types/jest": "^29.0.3",
    "jest": "^29.0.1",
    "parcel": "^2.7.0",
    "superwstest": "^2.0.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
