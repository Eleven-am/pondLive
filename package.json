{
  "name": "@eleven-am/pondlive",
  "version": "0.1.19",
  "description": "PondLive is a simple, lightweight, and easy to use Web framework",
  "keywords": [
    "socket",
    "server",
    "ws",
    "websocket",
    "pubsub",
    "presence",
    "realtime",
    "reactive",
    "realtime server",
    "serverside rendering"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --coverage --verbose --watchAll",
    "build": "tsc --project tsconfig.build.json",
    "publishToNpm": "npm version patch && npm publish",
    "mon:client": "parcel src/server/assets/index.html",
    "mon:server": "nodemon --watch src --exec ts-node src/server/subscriptions/createState.ts --trace-warnings",
    "dev": "concurrently \"npm run mon:server\" \"npm run mon:client\""
  },
  "author": "Roy OSSAI",
  "license": "GPL-3.0",
  "main": "./index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eleven-am/pondLive.git"
  },
  "dependencies": {
    "@eleven-am/pondsocket": "^0.1.60",
    "busboy": "^1.6.0",
    "parcel": "^2.8.3"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/node": "^16.10.3",
    "@types/websocket": "^1.0.5",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-plugin-file-progress": "^1.3.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.0.1",
    "nodemon": "^2.0.22",
    "process": "^0.11.10",
    "superwstest": "^2.0.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
